{
  "name": "misc-text-util",
  "main": "./lib/misc-text-util",
  "version": "0.3.0",
  "description": "Additional text manipulation functions such as: sorting, selected text from separation from line to comma or comma to line, block commenting a selection (/* ... */) and wrapping selection in double or single quotes.",
  "keywords": [
    "text-manipulation",
    "sort-text",
    "quote-wrapping",
    "comment-block",
    "text-wrapping"
  ],
  "activationCommands": {
    "atom-workspace": [
      "misc-text-tools:toggleCsvList",
      "misc-text-tools:sortSelectionAsc",
      "misc-text-tools:sortSelectionDesc",
      "misc-text-tools:singleQuoteListElements",
      "misc-text-tools:doubleQuoteListElements",
      "misc-text-tools:commentWrap",
      "misc-text-tools:parenthesisWrap",
      "misc-text-tools:curlybraceWrap",
      "misc-text-tools:blockSingleQuoteWrap",
      "misc-text-tools:blockDoubleQuoteWrap"
    ]
  },
  "repository": "https://github.com/KevinOnEarth/misc-text-util",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
